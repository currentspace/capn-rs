{
  "name": "capnweb-rust-interop-tests",
  "version": "1.0.0",
  "description": "TypeScript client integration tests for Cap'n Web Rust implementation",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "test": "npm run build && node dist/test-runner.js",
    "test:watch": "tsc --watch & nodemon dist/test-runner.js",
    "clean": "rm -rf dist",
    "start:rust-server": "cd .. && cargo run --example basic_server",
    "interop": "npm run build && node dist/interop-tests.js"
  },
  "dependencies": {
    "capnweb": "^0.1.0",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "@types/ws": "^8.5.12",
    "typescript": "^5.6.0",
    "nodemon": "^3.1.7"
  },
  "keywords": [
    "capnweb",
    "rpc",
    "rust",
    "typescript",
    "interoperability"
  ],
  "author": "Cap'n Web Rust Team",
  "license": "MIT OR Apache-2.0"
}