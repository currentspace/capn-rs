#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/brianmeek/scratch/capn-rs/typescript-interop/node_modules/.pnpm/capnweb@https+++codeload.github.com+cloudflare+capnweb+tar.gz+051358482556d577651ec8e893f4d4f007196851/node_modules/capnweb/node_modules/.pnpm/vitest@3.2.4_@types+node@24.5.2_@vitest+browser@3.2.4/node_modules/vitest/node_modules:/Users/brianmeek/scratch/capn-rs/typescript-interop/node_modules/.pnpm/capnweb@https+++codeload.github.com+cloudflare+capnweb+tar.gz+051358482556d577651ec8e893f4d4f007196851/node_modules/capnweb/node_modules/.pnpm/vitest@3.2.4_@types+node@24.5.2_@vitest+browser@3.2.4/node_modules:/Users/brianmeek/scratch/capn-rs/typescript-interop/node_modules/.pnpm/capnweb@https+++codeload.github.com+cloudflare+capnweb+tar.gz+051358482556d577651ec8e893f4d4f007196851/node_modules/capnweb/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/brianmeek/scratch/capn-rs/typescript-interop/node_modules/.pnpm/capnweb@https+++codeload.github.com+cloudflare+capnweb+tar.gz+051358482556d577651ec8e893f4d4f007196851/node_modules/capnweb/node_modules/.pnpm/vitest@3.2.4_@types+node@24.5.2_@vitest+browser@3.2.4/node_modules/vitest/node_modules:/Users/brianmeek/scratch/capn-rs/typescript-interop/node_modules/.pnpm/capnweb@https+++codeload.github.com+cloudflare+capnweb+tar.gz+051358482556d577651ec8e893f4d4f007196851/node_modules/capnweb/node_modules/.pnpm/vitest@3.2.4_@types+node@24.5.2_@vitest+browser@3.2.4/node_modules:/Users/brianmeek/scratch/capn-rs/typescript-interop/node_modules/.pnpm/capnweb@https+++codeload.github.com+cloudflare+capnweb+tar.gz+051358482556d577651ec8e893f4d4f007196851/node_modules/capnweb/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../vitest/vitest.mjs" "$@"
else
  exec node  "$basedir/../vitest/vitest.mjs" "$@"
fi
